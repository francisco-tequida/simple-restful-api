				{
					"name": "Generate token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Access token was not empty\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.access).not.eq(undefined);\r",
									"    \r",
									"    pm.environment.set(\"jwt_token\", jsonData.access);\r",
									"});\r",
									"\r",
									"pm.test(\"Successful token request\", function () {\r",
									"    pm.expect(pm.response.code).to.be.oneOf([200, 201, 202]);\r",
									"});\r",
									"\r",
									"pm.test(\"Response time is less than 200ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
									"});\r",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\r\n    \"username\": \"admin\",\r\n    \"password\": \"admin\"\r\n}"
						},
						"url": {
							"raw": "{{url}}/api/token/",
							"host": [
								"{{url}}"
							],
							"path": [
								"api",
								"token",
								""
							]
						},
						"description": "generate a valid token for a user"
					},
					"response": []
				},
